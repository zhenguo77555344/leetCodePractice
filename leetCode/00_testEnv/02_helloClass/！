#include<iostream>
using namespace std;
class Animal {
    public:
        int run();
    private:
        int m_Count;

};

int Animal::run(){

    cout<<"Animal can run"<<endl;
//    cout<<"address of run="<<&Animal::run<<endl;
    
    return 0;
}


int main(){
    Animal * am=new Animal;
    am->run();
    int i=0;
    cout<<"address of i="<<&i<<endl;
    
  //  cout<<"address of main="<<&main<<endl;
    cout<<"address of am="<<&am<<endl;
     return 0;

}
